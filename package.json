{
  "name": "Skid",
  "version": "1.0.0",
  "description": "Skid",
  "author": "@baturinsky",
  "type": "module",
  "scripts": {
    "start": "concurrently --kill-others \"esbuild src/prog.ts --bundle --outfile=public/bundle.js --watch\" \"http-server -p 5000 public\"",
    "build": "esbuild src/prog.ts --bundle --outfile=public/minified.js --minify && regpack public/minified.js > public/packed.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "regpack": "^5.0.1"
  }
}
